{
  "name": "@wispbit/sdk",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsx build.ts",
    "build:tsc": "tsc",
    "test": "vitest run --exclude '**/*eval.test.ts'",
    "lint": "eslint . --ext .ts",
    "deploy": "pnpm run build && pnpm publish --access=public --no-git-checks"
  },
  "dependencies": {
    "big.js": "^7.0.1",
    "date-fns": "^2.30.0",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "openai": "^4.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "vite-tsconfig-paths": "^5.1.4",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0"
  }
}
